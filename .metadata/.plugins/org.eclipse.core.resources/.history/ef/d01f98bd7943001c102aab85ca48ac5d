

public class Equals {

	public static void main(String[] args) {
		
		java.lang.String name="DEEPAVALI";
		
		String fesName=new String(" FESTIVAL");
		
		name.equals(fesName);
		System.out.println(name.equals(fesName));
		
		name.toLowerCase();
		System.out.println(name.toLowerCase());
		
		name.concat(fesName);
		System.out.println(name.concat(fesName));
		
		//String[] dish= {"Holige","Jilebi","Jamoon","Paysa"};
		// Britania
		// an,3
		System.out.println(fesName.lastIndexOf("ES",2));
	
	}

}
